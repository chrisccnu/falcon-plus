{
    "debug": false,
    "http": {
        "enabled": true,
        "listen": "%%GRAPH_HTTP%%"
    },
    "rpc": {
        "enabled": true,
        "listen": "%%GRAPH_RPC%%"
    },
    "rrd": {
        "storage": "./data/6070"
    },
    "db": {
        "dsn": "%%MYSQL%%/graph?loc=Local&parseTime=true",
        "maxIdle": 4
    },
    "callTimeout": 5000,
    "migrate": {
            "enabled": false,
            "concurrency": 2,
            "replicas": 500,
            "cluster": {
                    "graph-00" : "pm2.yunzhijia.com:6070",
                    "graph-00" : "pm1.yunzhijia.com:6070"
            }
    }
}
